<h1>My Tasks</h1>
<% @tasks.each do |task| %>
   <%= check_box("task", "@task.complete", {checked: task.completed , :multiple => true, :style => "margin-left: 16px;"}, "true", "false") %>
  <p><%= task.completed %> </p>
  <p><%= link_to task.title, task_path(task) %></p>
<% end %>

 <p><%= link_to "New task", new_task_path %></p>







